{
  "defaultSeverity": "error",
  "extends": [
      "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
      "no-console": false,
      "space-before-function-paren": [true, {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }],
      "semicolon": [true, "always"],
      "no-unused-expression": false,
      "quotemark": [true, "single", "jsx-double"]
  },
  "rulesDirectory": []
}
